{
  "private": true,
  "name": "website-fontainetkd",
  "version": "0.0.1",
  "author": "Axel Bocciarelli",
  "description": "Website of the Fontaine Taekwon-Do club.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelboc/website-fontainetkd"
  },
  "engines": {
    "node": "16.*",
    "yarn": "*"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:local": "set NODE_ENV=development && yarn build && yarn serve",
    "build:prod": "set NODE_ENV=production && yarn build && yarn serve",
    "serve": "gatsby serve -p 8000",
    "lint": "run-p lint:*",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:check": "npx stylelint-config-prettier-check",
    "lint:pretty": "prettier --check \"**/*.{js,jsx,css,json,md}\""
  },
  "dependencies": {
    "gatsby": "^3.10.1",
    "gatsby-plugin-image": "^1.10.1",
    "gatsby-plugin-nprogress": "^3.10.0",
    "gatsby-plugin-postcss": "^4.10.0",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^3.10.2",
    "gatsby-plugin-sitemap": "^4.6.0",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-sharp": "^3.10.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "smooth-scroll": "^16.1.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.15",
    "eslint": "^7.31.0",
    "eslint-config-galex": "^2.16.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.2",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.3.5"
  }
}
