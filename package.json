{
  "private": true,
  "name": "website-fontainetkd",
  "version": "0.0.1",
  "author": "Axel Bocciarelli",
  "description": "Website of the Fontaine Taekwon-Do club.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelboc/website-fontainetkd"
  },
  "engines": {
    "node": "18.*",
    "yarn": "*"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:local": "set NODE_ENV=development && yarn build && yarn serve",
    "build:prod": "set NODE_ENV=production && yarn build && yarn serve",
    "serve": "gatsby serve -p 8000 -o",
    "lint": "run-p lint:*",
    "lint:js": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "lint:tsc": "tsc",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:check": "npx stylelint-config-prettier-check",
    "lint:pretty": "prettier --check ."
  },
  "dependencies": {
    "gatsby": "5.11.0",
    "gatsby-plugin-image": "3.11.0",
    "gatsby-plugin-nprogress": "5.11.0",
    "gatsby-plugin-postcss": "6.11.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-sharp": "5.11.0",
    "gatsby-plugin-sitemap": "6.11.0",
    "gatsby-source-filesystem": "5.11.0",
    "gatsby-transformer-sharp": "5.11.0",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "smooth-scroll": "16.1.3"
  },
  "devDependencies": {
    "@types/react": "18.2.18",
    "@types/smooth-scroll": "16.1.1",
    "eslint": "^7.31.0",
    "eslint-config-galex": "^2.16.7",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.27",
    "postcss-browser-reporter": "0.6.0",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "9.1.1",
    "postcss-reporter": "7.0.5",
    "prettier": "3.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "5.1.6"
  }
}
