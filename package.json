{
  "private": true,
  "name": "website-fontainetkd",
  "version": "0.0.1",
  "author": "Axel Bocciarelli",
  "description": "Website of the Fontaine Taekwon-Do club.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelboc/website-fontainetkd"
  },
  "engines": {
    "node": "^12.14.1",
    "yarn": "*"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:local": "set NODE_ENV=development && yarn build && yarn serve",
    "build:prod": "set NODE_ENV=production && yarn build && yarn serve",
    "serve": "gatsby serve -p 8000",
    "lint": "run-p lint:*",
    "lint:js": "eslint \"**/*.js\"",
    "lint:js:check": "eslint --print-config . | eslint-config-prettier-check",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:check": "stylelint-config-prettier-check",
    "lint:pretty": "prettier --check \"**/*.{js,jsx,ts,tsx,css,json,md}\""
  },
  "dependencies": {
    "gatsby": "^2.19.10",
    "gatsby-plugin-anchor-links": "^1.0.11",
    "gatsby-plugin-nprogress": "^2.1.18",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sitemap": "^2.2.26",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0-beta",
    "react-icons": "^3.9.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-problems": "^4.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^4.0.0",
    "typescript": "^3.7.5"
  }
}
