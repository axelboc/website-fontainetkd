{
  "private": true,
  "name": "website-fontainetkd",
  "version": "0.0.1",
  "author": "Axel Bocciarelli",
  "description": "Website of the Fontaine Taekwon-Do club.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelboc/website-fontainetkd"
  },
  "type": "module",
  "engines": {
    "node": "24.*",
    "yarn": "*"
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:local": "set NODE_ENV=development && yarn build && yarn serve",
    "build:prod": "set NODE_ENV=production && yarn build && yarn serve",
    "serve": "gatsby serve -p 8000 -o",
    "lint": "run-p lint:*",
    "lint:js": "eslint --max-warnings=0",
    "lint:tsc": "tsc",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:check": "npx stylelint-config-prettier-check",
    "lint:pretty": "prettier --check --cache ."
  },
  "dependencies": {
    "gatsby": "5.14.6",
    "gatsby-plugin-image": "3.14.0",
    "gatsby-plugin-nprogress": "5.14.0",
    "gatsby-plugin-postcss": "6.14.0",
    "gatsby-plugin-react-svg": "3.3.0",
    "gatsby-plugin-sharp": "5.14.0",
    "gatsby-plugin-sitemap": "6.14.0",
    "gatsby-source-filesystem": "5.14.0",
    "gatsby-transformer-sharp": "5.14.0",
    "normalize.css": "8.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.5.0",
    "smooth-scroll": "16.1.3"
  },
  "devDependencies": {
    "@esrf/eslint-config": "1.3.3",
    "@types/react": "18.3.12",
    "@types/smooth-scroll": "16.1.4",
    "eslint": "9.33.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.6",
    "postcss-browser-reporter": "0.7.0",
    "postcss-import": "16.1.1",
    "postcss-preset-env": "10.2.4",
    "postcss-reporter": "7.1.0",
    "prettier": "3.6.2",
    "stylelint": "16.23.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "39.0.0",
    "stylelint-order": "7.0.0",
    "typescript": "5.9.2"
  }
}
